    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>A react component that displays a spinning waiting icon.</description>
        <global>false</global>
        <name>x_snc_reactor.Waiting</name>
        <script><![CDATA[class Waiting extends ReactorComponent
{
	constructor(props)
	{
		super(props);
	}
	
	render()
	{
		var strText = this.props.text;
		return e('div',{},[
			e(Icon,{
				icon: 'spinner',
				spinning: true,
				size: 2 }),
			e('span',{},strText)
			]);	
	}	
}]]></script>
