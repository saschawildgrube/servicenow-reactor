<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_script">
    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Produces a container that spans over the complete available canvas.</description>
        <global>false</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_reactor.Fullscreen</name>
        <script><![CDATA[class Fullscreen extends ReactorComponent
{
	constructor(props)
	{
		super(props);
	}

	render()
	{
		return e('div',
			{
				style: {
					position: 'absolute',
					display: 'block',
					height: '99%',
					width: '99%'
				}
			},
			this.props.children);
	}	
}]]></script>
        <script_name>Fullscreen</script_name>
