        <description>A React component that creates a button that can be clicked.</description>
        <jsx>false</jsx>
        <name>Button</name>
        <role>element</role>
        <script><![CDATA[class Button extends React.Component
{
	constructor(props)
	{
		super(props);
		this.onClick = this.onClick.bind(this);
	}
	
	onClick()
	{
		if (typeof this.props.callbackOnClick == 'function')
		{
			this.props.callbackOnClick();
		}
	}
	
	render()
	{
		return e('button',
			{
				onClick: this.onClick
			},
			this.props.text);	
	}	
}]]></script>
